

 <h1>Task Name:  </h1>
    <%= @add_link = '<a href="' + "../../#{@task.id}" + '">' + "#{@task.title}" + '</a>' %>

 <h1> Subtask Name:  </h1>
 <%= @subtask.title %>



 <div>
   <h1>Subtask Description: </h1>
    <p> Estimated Duration: <%= @subtask.estimated_duration %></p>
    <p> Deadline: <%= @subtask.priority %></p>
    <p> Notes: <%= @subtask.notes %></p>
    <p>The following user(s) is/are assigned to this task:</p>
      <ul>
         <% @subtask.users.each do |user| %>
           <li><%= user.username %></li>
        <% end%>
      </ul>
 </div>




 <div>
   <%= @form_open = '<form action="' + "../../#{@task.id}/subtasks/#{@subtask.id}/delete"  + '" method="POST"' + '>' %>
     <input id="hidden" type="hidden" name="_method" value="DELETE">
     <input type="submit" value="Delete Subtask">
     </form>
 </div>

 <div>
   <%= @add_link = '<a href="' + "../../#{@task.id}/subtasks/#{@subtask.id}/edit" + '">' + "Edit Subtask" + '</a>' %>
 </div>
