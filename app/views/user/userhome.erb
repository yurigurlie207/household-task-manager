<h1>Here are your assigned subtasks:</h1>
<ul>
  <% if @usertasks %>
    <% @usertasks.each do |usertask| %>
    <% subtask = Subtask.find_by_id(usertask.subtask_id) %>
      <li><%= @add_link = '<a href="' + "../tasks/#{subtask.task_id}/subtasks/#{subtask.id}" + '">' + "#{subtask.title}" + '</a>' %>
      - <% if subtask.complete == true %>
         Complete
      <% else %>
        Incomplete
      <% end %>
    </li>
    <% end %>
  <% end %>
</ul>



<a href= "../tasks/new">Create New Task</a><br>
<%= @add_link = '<a href="' + "../user/#{@user.id}" + '">' + "My Profile" + '</a>' %><br>
<a href= "../user/logout">Log Out</a>
